D.4000: DEFB    "AB"
D$4002: DEFW    J409F           ; INIT
        DEFW    J4161           ; STATEMENT
        DEFW    J424E           ; DEVICE
        DEFW    0               ; BASIC
        DEFS    6,0

;
; BIOS table for RS232
;

;     Device information byte indicates following options are installed or not:
;       bits #  76543210
;               ||||||||
;               |||||||+----- reserved
;               |||||||
;               ||||||+------ TXREADY interrupt
;               ||||||
;               |||||+------- sync/break character detected
;               |||||
;               ||||+-------- timer interrupt
;               ||||
;               |||+--------- carrier detect
;               |||
;               ||+---------- ring indicator
;               ||
;               |+----------- reserved
;               |
;               +------------ reserved

I4010:  DEFB    034H                    ; +00 MSX serial features (no TxReady INT, Sync detect, No Timer INT, Have CD, Have RI)
        DEFB    002H                    ; +01 MSX serial version (version 3.0)
        DEFB    0                       ; +02 reserved

        JP      J406B                   ; +03 RS232.INIT
?4016:  JP      C42E4                   ; +06 RS232.OPEN
?4019:  JP      C4F56                   ; +09 RS232.STAT
?401C:  JP      C4372                   ; +0C RS232.GETCHR
?401F:  JP      C4FF1                   ; +0F RS232.SNDCHR
?4022:  JP      C4338                   ; +12 RS232.CLOSE
?4025:  JP      J4094                   ; +15 RS232.EOF
?4028:  JP      C43CC                   ; +18 RS232.LOC
?402B:  JP      J43FC                   ; +1B RS232.LOF
?402E:  JP      C443F                   ; +1E RS232.BACKUP
?4031:  JP      C4FB9                   ; +21 RS232.SNDBRK
?4034:  JP      C506F                   ; +24 RS232.DTR
?4037:  JP      J4040                   ; +27 RS232.SETCHN
?403A:  JP      J4048                   ; +2A RS232.????
?403D:  JP      J405D                   ; +2D RS232.????

;        Subroutine RS232.SETCHN
;           Inputs  ________________________
;           Outputs ________________________

J4040:  CP      2
        CCF
        RET     C
        LD      (D.6031),A              ; select channel
        RET

;        Subroutine RS232.????
;           Inputs  ________________________
;           Outputs ________________________

J4048:  PUSH    BC
        PUSH    DE
        PUSH    HL
        OR      A
        LD      A,0
        JR      Z,J4055
        CALL    C56B5
        JR      J4058
J4055:  CALL    C56C1
J4058:  POP     HL
        POP     DE
        POP     BC
        EI
        RET

;        Subroutine RS232.????
;           Inputs  ________________________
;           Outputs ________________________

J405D:  PUSH    BC
        PUSH    DE
        PUSH    HL
        LD      A,(D.6031)              ; current channel
        CALL    C5773
        POP     HL
        POP     DE
        POP     BC
        EI
        RET

;        Subroutine RS232.INIT
;           Inputs  ________________________
;           Outputs ________________________

J406B:  PUSH    BC
        PUSH    DE
I406D:  PUSH    HL
        LD      IY,-13
        ADD     IY,SP
        LD      SP,IY
        PUSH    IY
        POP     DE
        LD      C,13
J407B:  CALL    C4565
        LD      (DE),A
        INC     DE
        DEC     C
        JR      NZ,J407B
        CALL    C4D7C                   ; initialize RS232 port
        PUSH    AF
        LD      IY,-13-2
        ADD     IY,SP
        POP     AF
        LD      SP,IY
        POP     HL
        POP     DE
        POP     BC
        RET

;        Subroutine RS232.EOF
;           Inputs  ________________________
;           Outputs ________________________

J4094:  LD      HL,D.0000
        CALL    C4EDB
        RET     Z
        CALL    C441F
        RET

;
; CARTRIDGE INIT
;

J409F:  CALL    C5518                   ; test at which port
        CALL    C54DD
        LD      IY,I4154
        CALL    C4D7C                   ; initialize RS232 port
        LD      A,1
        LD      (D.6031),A              ; select channel 1
        LD      IY,I4154
        CALL    C4D7C                   ; initialize RS232 port
        LD      HL,HOKVLD
        BIT     0,(HL)                  ; EXTBIO valid ?
        JR      NZ,J40CB                ; yep,
        SET     0,(HL)
        LD      HL,EXTBIO
        LD      B,05H
J40C6:  LD      (HL),0C9H
        INC     HL
        DJNZ    J40C6                   ; initialize EXTBIO
J40CB:  XOR     A
        LD      DE,0801H
        CALL    EXTBIO                  ; get rs232 channelnumber for my first channel
        LD      HL,D.FB19
        LD      (HL),A
        XOR     A
        LD      DE,0001H
        CALL    EXTBIO                  ; get my trap entry
        ADD     A,A
        ADD     A,A
        ADD     A,A
        ADD     A,A
        OR      (HL)
        LD      (HL),A
        LD      DE,OLDSTT+3
        DI
        LD      HL,EXTBIO
        LD      BC,5
        LDIR
        LD      DE,J.FB14
        LD      HL,H.NEWS
        LD      BC,5
        LDIR
        CALL    C571F                   ; get slotid at address
        LD      (EXTBIO+1),A
        LD      (H.NEWS+1),A
        LD      A,0F7H
        LD      (EXTBIO+0),A
        LD      (H.NEWS+0),A
        LD      HL,I4493
        LD      (EXTBIO+2),HL
        LD      HL,I4C6D
        LD      (H.NEWS+2),HL
        LD      A,0C9H
        LD      (EXTBIO+4),A
        LD      (H.NEWS+4),A
        LD      BC,24
        LD      HL,I412C
        LD      DE,DISINT
        LDIR
        EI
        RET

I412C:  PUSH    DE
        LD      E,02H
        JR      J4134
?4131:  PUSH    DE
        LD      E,03H
J4134:  LD      D,00H
        PUSH    IX
        PUSH    IY
        CALL    EXTBIO
        EI
        POP     IY
        POP     IX
        POP     DE
        RET

;        Subroutine __________________________
;           Inputs  ________________________
;           Outputs ________________________

C4144:  XOR     A
        CALL    C50B4
;

;        Subroutine __________________________
;           Inputs  ________________________
;           Outputs ________________________

C4148:  PUSH    BC
        PUSH    DE
        PUSH    HL
        XOR     A                       ; channel 0
        CALL    C5773
        POP     HL
        POP     DE
        POP     BC
        EI
        RET

I4154: DEFB    "8N1XHNNN"
        DEFW    1200
        DEFW    1200
        DEFB    0

;
; STATEMENT
;

J4161:  EI
        PUSH    HL
        POP     IX
        LD      DE,I41B0
        CALL    C416E
        RET     C
        PUSH    DE
        RET

;        Subroutine __________________________
;           Inputs  ________________________
;           Outputs ________________________

C416E:  PUSH    BC
        PUSH    HL
J4170:  LD      HL,PROCNM
        LD      A,(DE)
        INC     A
        JR      Z,J4189
        CALL    C418D
        JR      Z,J4181
        INC     DE
        INC     DE
        INC     DE
        JR      J4170
J4181:  EX      DE,HL
        INC     HL
        LD      E,(HL)
        INC     HL
        LD      D,(HL)
        AND     A
        JR      J418A
J4189:  SCF
J418A:  POP     HL
        POP     BC
        RET

;        Subroutine __________________________
;           Inputs  ________________________
;           Outputs ________________________

C418D:  LD      A,(DE)
        CALL    C4D73                   ; to uppercase
        LD      C,A
        LD      A,(HL)
        CALL    C4D73                   ; to uppercase
        CP      C
        JR      NZ,J419F
        AND     A
        RET     Z
        INC     DE
        INC     HL
        JR      C418D
J419F:  LD      A,(DE)
        AND     A
        JR      Z,J41A6
        INC     DE
J41A4:  JR      J419F
J41A6:  LD      A,(HL)
        AND     A
        JR      Z,J41AD
        INC     HL
        JR      J41A6
J41AD:  LD      A,L
        OR      H
        RET

I41B0:  DEFB    "COMINI",0
        DEFW    J4632
        DEFB    "COMDTR",0
        DEFW    J46E7
        DEFB    "COMSTAT",0
        DEFW    J4704
        DEFB    "COMBREAK",0
        DEFW    J4728
        DEFB    "COMTERM",0
        DEFW    J476C
        DEFB    "COM",0
        DEFW    J4BC4
        DEFB    "COMON",0
        DEFW    J4C1C
        DEFB    "COMOFF",0
        DEFW    J4BFE
        DEFB    "COMSTOP",0
        DEFW    J4C3A
        DEFB    "COMHELP",0
        DEFW    J49C6
        DEFB    "DOS",0
        DEFW    J421C
        DEFB    0FFH

I4213:  DEFB    ":"
        DEFB    "_SYSTEM"
        DEFB    0

; DOS

J421C:  CALL    C5754
        DI
        LD      HL,0
        LD      (D.4000),HL
        LD      HL,(D.603A)
        LD      (HL),0
        LD      HL,OLDSTT+3
        LD      DE,EXTBIO
        LD      BC,5
        LDIR
        LD      DE,H.NEWS
        LD      BC,5
        LDIR
        LD      HL,I4213
        LD      DE,BUF
        LD      BC,I$0009
        LDIR
        LD      HL,BUF
        EI
        RET

;
; DEVICE
;

J424E:  EI
        CP      0FFH			; device inquiry ?
        JP      NZ,J4295                ; nope, handle device function
        LD      HL,PROCNM
        LD      A,(HL)
        CP      "C"
        JR      NZ,J4292
        INC     HL
        LD      A,(HL)
        CP      "O"
        JR      NZ,J4292
        INC     HL
        LD      A,(HL)
        CP      "M"
        JR      NZ,J4292
        INC     HL
        LD      A,(HL)
        AND     A
        JR      NZ,J4270
        DEC     HL
        LD      A,"0"
J4270:  SUB     "0"
        JR      C,J4292
        CP      9+1
        JR      NC,J4292
        PUSH    BC
        PUSH    AF
        LD      A,(D.FB19)
        AND     0FH
        LD      B,A
        POP     AF
        CP      B
        JR      Z,J4288
        INC     B
        CP      B
        JR      NZ,J4292
J4288:  LD      (D.6031),A              ; select channel
        POP     BC
        INC     HL
        LD      A,(HL)
        AND     A
        RET     Z
        SCF
        RET
J4292:  POP     BC
        SCF
        RET
J4295:  PUSH    HL
        PUSH    AF
        OR      A
        JR      Z,J42AF
        PUSH    AF
        PUSH    DE
        EX      DE,HL
        CALL    C508A
        OR      A
        SBC     HL,DE
        JR      Z,J42AD
        LD      A,(D.6031)
        XOR     01H
        LD      (D.6031),A              ; switch channel
J42AD:  POP     DE
        POP     AF
J42AF:  LD      HL,I42BE
        ADD     A,L
        LD      L,A
        JR      NC,J42B7
        INC     H
J42B7:  LD      A,(HL)
        INC     HL
        LD      H,(HL)
        LD      L,A
        POP     AF
        EX      (SP),HL
        RET

I42BE:  DEFW    C42D2                   ; BASIC device open
        DEFW    C4338                   ; BASIC device close
        DEFW    C451E                   ; BASIC device random I/O
        DEFW    C435F                   ; BASIC device sequential output
        DEFW    C436B                   ; BASIC device sequential input
        DEFW    C43B9                   ; BASIC device loc function
        DEFW    C43F1                   ; BASIC device lof function
        DEFW    C440E                   ; BASIC device eof function
        DEFW    C453C                   ; BASIC device fpos function
        DEFW    C443F                   ; BASIC device backup a character

;        Subroutine BASIC device open
;           Inputs  ________________________
;           Outputs ________________________

C42D2:  LD      C,7FH
        CALL    C42E4                   ; RS232.OPEN
        JR      C,J42DD
        LD      (PTRFIL),HL
        RET
J42DD:  DEC     A
        JP      Z,J4512
        JP      J4524

;        Subroutine RS232.OPEN
;           Inputs  ________________________
;           Outputs ________________________

C42E4:  CALL    C50EA
        OR      A
        BIT     3,A
        LD      A,02H   ; 2 
        SCF
        RET     NZ
        LD      A,E
        CP      08H     ; 8 
        LD      A,01H   ; 1 
        SCF
        RET     Z
        PUSH    BC
        PUSH    DE
        PUSH    HL
        LD      A,(D.6031)
        OR      A                       ; current channel 0 ?
        JR      Z,J4303                 ; yep,
        LD      (OLDSTT+0),HL
        JR      J4306
J4303:  LD      (MEXBIH+3),HL
J4306:  LD      A,C
        LD      (OLDSTT+2),A
        LD      (HL),E
        XOR     A
        INC     HL
        INC     HL
        LD      (HL),A
        INC     HL
        LD      (HL),A
        INC     HL
        INC     HL
        INC     HL
        LD      (HL),A
        CALL    C4144
        CALL    C509B
        SET     3,(HL)
        LD      HL,D.601B
        LD      A,(D.6031)
        OR      A                       ; current channel 0 ?
        JR      Z,J432A                 ; yep,
        SET     1,(HL)
        JR      J432C
J432A:  SET     0,(HL)
J432C:  DI
        CALL    C4F9A
        CALL    C4F1E
        OR      A
        POP     HL
        POP     DE
        POP     BC
        RET

;        Subroutine RS232.CLOSE
;           Inputs  ________________________
;           Outputs ________________________

C4338:  PUSH    BC
        PUSH    DE
        PUSH    HL
        CALL    C508A
        LD      A,(HL)
        CP      02H     ; 2 
        SCF
        CCF
        JR      NZ,J434A
        LD      A,1AH
D$4346  EQU     $-1
        CALL    C4FF1                   ; RS232.SNDCHR
J434A:  DI

;        Subroutine __________________________
;           Inputs  ________________________
;           Outputs ________________________

C434B:  CALL    C4F2C
D$434C  EQU     $-2
        LD      A,(D.6031)              ; current channel
        CALL    C5768
        EI
        CALL    C509B
        RES     3,(HL)
        POP     HL
        POP     DE
        POP     BC
        EI
        RET

;        Subroutine BASIC device sequential output
;           Inputs  ________________________
;           Outputs ________________________

C435F:  LD      A,C
        CALL    C4FF1                   ; RS232.SNDCHR
        EI
        JP      C,J4548
        JP      Z,J4548
        RET

;        Subroutine BASIC device sequential input
;           Inputs  ________________________
;           Outputs ________________________

C436B:  CALL    C4372                   ; RS232.GETCHR
        JP      M,J4548
        RET

;        Subroutine RS232.GETCHR
;           Inputs  ________________________
;           Outputs ________________________

C4372:  PUSH    HL
        PUSH    BC
        CALL    C4457
        LD      C,(HL)
        LD      (HL),00H
        DEC     HL
        LD      A,(HL)
        CALL    C50D8
        LD      (HL),00H
        INC     HL
        AND     A
        JR      NZ,J43AF
        LD      A,C
        AND     A
        JR      NZ,J439B
        PUSH    HL
        CALL    C4EF8
        LD      C,A
        PUSH    AF
        CALL    C504A
        POP     AF
        POP     HL
        JP      M,J43AF
        JR      C,J43AF
        JR      Z,J43AF
J439B:  PUSH    HL
        CALL    C508A
        LD      A,(HL)
        POP     HL
        CP      04H     ; 4 
        JR      Z,J43B3
        LD      A,C
        CP      1AH
        JR      NZ,J43B3
        LD      (HL),A
        AND     A
        SCF
        JR      J43B6
J43AF:  OR      80H
        JR      J43B5
J43B3:  XOR     A
        INC     A
J43B5:  LD      A,C
J43B6:  POP     BC
        POP     HL
        RET

;        Subroutine BASIC device loc function
;           Inputs  ________________________
;           Outputs ________________________

C43B9:  PUSH    HL
        PUSH    AF
        LD      A,(D.6031)              ; current channel
        CALL    C5460
        POP     AF
J43C2:  LD      (DAC+2),HL
        LD      HL,VALTYP
        LD      (HL),02H        ; 2 
        POP     HL
