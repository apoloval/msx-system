D.FB1A  EQU     0FB1AH  ; --SLI
D.FB1B  EQU     0FB1BH  ; --SL-
D.FB1C  EQU     0FB1CH  ; --SLI
D.FB1D  EQU     0FB1DH  ; --SL-
D.FB1E  EQU     0FB1EH  ; mode flag channel 0
